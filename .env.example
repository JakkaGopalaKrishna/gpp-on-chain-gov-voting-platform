# Example environment variables for local development

# Your private key for deploying to a test network (use a test wallet only)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY

# RPC URL for a test network (e.g., Sepolia via Infura/Alchemy)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Etherscan API key (for contract verification)
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY

# Local Hardhat node RPC URL (used by frontend when running via Docker)
NEXT_PUBLIC_RPC_URL=http://localhost:8545

# Deployed contract addresses (populate after deployment)
NEXT_PUBLIC_GOVERNOR_ADDRESS=
NEXT_PUBLIC_TOKEN_ADDRESS=
PRIVATE_KEY=0xabc123
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_KEY
ETHERSCAN_API_KEY=YOUR_KEY

NEXT_PUBLIC_RPC_URL=http://localhost:8545
NEXT_PUBLIC_GOVERNOR_ADDRESS=
NEXT_PUBLIC_TOKEN_ADDRESS=
